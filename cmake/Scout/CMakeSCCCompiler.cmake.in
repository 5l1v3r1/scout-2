SET(CMAKE_SCC_COMPILER "@CMAKE_SCC_COMPILER@")
SET(CMAKE_SCC_COMPILER_ARG1 "@CMAKE_SCC_COMPILER_ARG1@")
SET(CMAKE_SCC_COMPILER_ID "@CMAKE_SCC_COMPILER_ID@")
SET(CMAKE_SCC_COMPILER_VERSION "@CMAKE_SCC_COMPILER_VERSION@")
SET(CMAKE_SCC_PLATFORM_ID "@CMAKE_SCC_PLATFORM_ID@")
@SET_MSVC_SCC_ARCHITECTURE_ID@
SET(CMAKE_AR "@CMAKE_AR@")
SET(CMAKE_RANLIB "@CMAKE_RANLIB@")
SET(CMAKE_LINKER "@CMAKE_LINKER@")
SET(CMAKE_SCC_COMPILER_LOADED 1)

SET(CMAKE_SCC_COMPILER_ENV_VAR "SCC")

SET(CMAKE_SCC_COMPILER_ID_RUN 1)
SET(CMAKE_SCC_IGNORE_EXTENSIONS inl;h;hpp;HPP;H;o;O;obj;OBJ;def;DEF;rc;RC)
SET(CMAKE_SCC_SOURCE_FILE_EXTENSIONS sc;c)
SET(CMAKE_SCC_LINKER_PREFERENCE 30)
SET(CMAKE_SCC_LINKER_PREFERENCE_PROPAGATES 1)

# Save compiler ABI information.
SET(CMAKE_SCC_SIZEOF_DATA_PTR "@CMAKE_SCC_SIZEOF_DATA_PTR@")
SET(CMAKE_SCC_COMPILER_ABI "@CMAKE_SCC_COMPILER_ABI@")
SET(CMAKE_SCC_LIBRARY_ARCHITECTURE "@CMAKE_SCC_LIBRARY_ARCHITECTURE@")

IF(CMAKE_SCC_SIZEOF_DATA_PTR)
  SET(CMAKE_SIZEOF_VOID_P "${CMAKE_SCC_SIZEOF_DATA_PTR}")
ENDIF(CMAKE_SCC_SIZEOF_DATA_PTR)

IF(CMAKE_SCC_COMPILER_ABI)
  SET(CMAKE_INTERNAL_PLATFORM_ABI "${CMAKE_SCC_COMPILER_ABI}")
ENDIF(CMAKE_SCC_COMPILER_ABI)

IF(CMAKE_SCC_LIBRARY_ARCHITECTURE)
  SET(CMAKE_LIBRARY_ARCHITECTURE "@CMAKE_SCC_LIBRARY_ARCHITECTURE@")
ENDIF()

SET(CMAKE_SCC_HAS_ISYSROOT "@CMAKE_SCC_HAS_ISYSROOT@")
@CMAKE_SCC_OSX_DEPLOYMENT_TARGET_FLAG_CODE@


SET(CMAKE_SCC_IMPLICIT_LINK_LIBRARIES "@CMAKE_SCC_IMPLICIT_LINK_LIBRARIES@")
SET(CMAKE_SCC_IMPLICIT_LINK_DIRECTORIES "@CMAKE_SCC_IMPLICIT_LINK_DIRECTORIES@")

# get TRY_COMPILE() to link with mpi if enabled
SET(LINK_LIBRARIES "@SCOUT_MPI_LIBRARIES@")

@SET_CMAKE_CMCLDEPS_EXECUTABLE@
@SET_CMAKE_CL_SHOWINCLUDE_PREFIX@
